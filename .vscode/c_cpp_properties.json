{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "${env:SDK_ROOT}components/**",
                "${env:SDK_ROOT}modules/**",
                "${env:GNU_INSTALL_ROOT}/../arm-none-eabi/include"
            ],
            "defines": [
                "__STATIC_INLINE",
                "BOARD_CUSTOM"
            ],
            "compilerPath": "${env:GNU_INSTALL_ROOT}/arm-none-eabi-gcc",
            "cStandard": "gnu17",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "linux-gcc-arm",
            "configurationProvider": "ms-vscode.makefile-tools"
        }
    ],
    "version": 4
}